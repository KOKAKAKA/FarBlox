"SendNotification",
{
Title = "Freeze Spammer Disabled",
Text = "The Freeze Spammer is now disabled.",
Icon = "rbxassetid://15085662449", -- Replace with your icon asset ID
Duration = 1 -- Notification display duration (in seconds)
}
)
end
end

